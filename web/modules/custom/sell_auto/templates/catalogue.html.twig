
{{ vehicules }}

<div class="row text-center slideanim">
  {% for vehicule in vehicules %}
   <div class="col-sm-4">
     <div class="thumbnail">
       <a href="{{ path('sell_auto.view', {'id': vehicule.id }) }}">
         <img src="{{ vehicule.url }}" alt="Paris" width="400" height="300">
         <p><strong>Paris</strong></p>
         <p>Yes, we built Paris</p>
       </a>
     </div>
   </div>
 {% endfor %}
</div>
